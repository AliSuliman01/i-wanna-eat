{
    "openapi": "3.0.0",
    "info": {
        "title": "Wanna Eat API",
        "description": "L5 Swagger OpenApi description",
        "contact": {
            "email": "alisulimanq@gmail.com"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/api/auth/signup": {
            "post": {
                "tags": [
                    "auth"
                ],
                "summary": "Signup using email and password",
                "operationId": "SignupUser",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "username": {
                                        "type": "string",
                                        "example": "john lay"
                                    },
                                    "email": {
                                        "type": "string",
                                        "format": "email",
                                        "example": "user@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "PassWord12345"
                                    },
                                    "mobile_number": {
                                        "type": "string",
                                        "example": "0987654321"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "successful",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "isSuccessful": {
                                            "type": "boolean",
                                            "example": "true"
                                        },
                                        "hasContent": {
                                            "type": "boolean",
                                            "example": "true"
                                        },
                                        "code": {
                                            "type": "integer",
                                            "example": "200"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": ""
                                        },
                                        "data": {
                                            "$ref": "#/components/schemas/User"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "isSuccessful": {
                                            "type": "boolean",
                                            "example": "true"
                                        },
                                        "hasContent": {
                                            "type": "boolean",
                                            "example": "true"
                                        },
                                        "code": {
                                            "type": "integer",
                                            "example": "200"
                                        },
                                        "message": {
                                            "type": "string",
                                            "example": "The email has already been taken."
                                        },
                                        "data": {
                                            "type": "object",
                                            "example": "null"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/users": {
            "get": {
                "tags": [
                    "users"
                ],
                "summary": "Get list of users",
                "description": "Returns list of users",
                "operationId": "getUsersList",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Bad request"
                    }
                },
                "security": [
                    {
                        "api_key_security_example": []
                    }
                ]
            }
        },
        "/api/users/show/{id}": {
            "get": {
                "tags": [
                    "users"
                ],
                "summary": "Get one user",
                "description": "Returns one user",
                "operationId": "getUser",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Bad request"
                    }
                },
                "security": [
                    {
                        "api_key_security_example": []
                    }
                ]
            }
        },
        "/api/users/update": {
            "post": {
                "tags": [
                    "users"
                ],
                "summary": "update user",
                "description": "update user",
                "operationId": "UpdateUser",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Bad request"
                    }
                },
                "security": [
                    {
                        "api_key_security_example": []
                    }
                ]
            }
        },
        "/api/users/destroy": {
            "post": {
                "tags": [
                    "users"
                ],
                "summary": "destroy user",
                "description": "destroy user",
                "operationId": "DestroyUser",
                "responses": {
                    "200": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "Bad request"
                    }
                },
                "security": [
                    {
                        "api_key_security_example": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "User": {
                "required": [
                    "email",
                    "password"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "username": {
                        "type": "string",
                        "example": "username"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "user@test.com"
                    },
                    "email_verified_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-3-25 20:30:50"
                    },
                    "user_photo": {
                        "type": "string",
                        "example": "/storage/users/photos/username_1249382727.jpg"
                    },
                    "mobile_number": {
                        "type": "string",
                        "example": "0987654321"
                    },
                    "password_reset_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-3-25 20:30:50"
                    },
                    "language_id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "role_id": {
                        "type": "integer",
                        "example": "1"
                    }
                },
                "type": "object",
                "xml": {
                    "name": "User"
                }
            }
        }
    },
    "security": [
        []
    ]
}