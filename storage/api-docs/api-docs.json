                        "example": "1"
                    },
                    "username": {
                        "type": "string",
                        "example": "username"
                    },
                    "email": {
                        "type": "string",
                        "format": "email",
                        "example": "user@test.com"
                    },
                    "email_verified_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-3-25 20:30:50"
                    },
                    "user_photo": {
                        "type": "string",
                        "example": "/storage/users/photos/username_1249382727.jpg"
                    },
                    "mobile_number": {
                        "type": "string",
                        "example": "0987654321"
                    },
                    "password_reset_at": {
                        "type": "string",
                        "format": "date-time",
                        "example": "2020-3-25 20:30:50"
                    },
                    "language_id": {
                        "type": "integer",
                        "example": "1"
                    },
                    "role_id": {
